{
    "name": "goldfinch/loadable",
    "description": "Load more implementation for Silverstripe with front-end component",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "loadmore",
        "loadable",
        "loadmore-silverstripe",
        "loadable-silverstripe",
        "load-more",
        "loadable-dataobject",
        "loadmore-dataobject"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Art Karasev",
            "email": "art.karasev@proton.me"
        }
    ],
    "require": {
        "php": ">=8.0",
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0",
        "goldfinch/helpers": "^1.0"
    },
    "extra": {
        "expose": ["client/dist"]
    },
    "autoload": {
        "psr-4": {
            "Goldfinch\\Loadable\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
